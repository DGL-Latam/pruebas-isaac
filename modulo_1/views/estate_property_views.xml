<?xml version="1.0"?>
<odoo>

    <record id="estate_property_view_tree" model="ir.ui.view">
        <field name="name">estate.property.tree</field>
        <field name="model">estate.property</field>
        <field name="arch" type="xml">
            <tree string="Tests">
                <field name="name" width="100px"/>
                <field name="postcode" width="10px"/>
                <field name="bedrooms" width="20px"/>
                <field name="living_areas"/>
                <field name="expected_price" width="30px"/>
                <field name="selling_price" width="30px"/>
                <field name="date_availability"/>
            </tree>
        </field>
    </record>

    <record id="estate_property_action" model="ir.actions.act_window">
        <field name="name">Estate Property</field>
        <field name="res_model">estate.property</field>
        <field name="view_mode">tree,form</field>

    </record>

    <menuitem
            id="menu_estate_property_action"
            action="estate_property_action"
            name="Estate Property Action"
            groups="base.group_user"
            sequence="10"/>

</odoo>