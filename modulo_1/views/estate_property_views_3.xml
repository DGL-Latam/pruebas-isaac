<?xml version="1.0"?>
<odoo>

    <!-- Lista -->
    <record id="estate_property_tag_view_tree" model="ir.ui.view">
        <field name="name">estate.property.tag.tree</field>
        <field name="model">estate.property.tag</field>
        <field name="arch" type="xml">
            <tree string="Tests">
                <field name="name"/>
            </tree>
        </field>
    </record>

    <!-- Formato -->
    <record id="estate_property_tag_view_form" model="ir.ui.view">
        <field name="name">estate.property.tag.form</field>
        <field name="model">estate.property.tag</field>
        <field name="arch" type="xml">
            <form string="House">
                <sheet>
                    <group>
                        <group>
                            <field name="name"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <!-- Inicio -->
    <record id="estate_property_action_tag" model="ir.actions.act_windows">
        <field name="name">Estate Property Tag</field>
        <field name="res_model">estate.property.tag</field>
        <field name="view.mode">tree,form</field>
    </record>

    <menuitem id="menu_estate_property_action_3_1" action="estate_property_action_tag" name="Property Tag" parent="menu_estate_property_action_3"
              groups="base.group_user" sequence="60"/>
</odoo>